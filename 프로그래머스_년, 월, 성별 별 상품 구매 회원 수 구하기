SELECT TO_CHAR(OS.SALES_DATE, 'YYYY') AS YEAR,
    TO_NUMBER(TO_CHAR(OS.SALES_DATE, 'MM')) AS MONTH,  -- MONTH 출력 형식 맞게, EXTRACT나 TO_NUMBER 사용
    GENDER,
    COUNT(DISTINCT UI.USER_ID) AS USERS  -- ID 중복 처리
    -- (SELECT COUNT(TO_CHAR(OS.SALES_DATE, 'YYYY'), TO_CHAR(OS.SALES_DATE, 'MM'), GENDER) 
    --  FROM JOIN ONLINE_SALE OS)
FROM USER_INFO UI
INNER JOIN ONLINE_SALE OS  -- LEFT JOIN 제외 (판매 안됐을수 있으므로?)
ON UI.USER_ID = OS.USER_ID
WHERE GENDER IS NOT NULL
GROUP BY TO_CHAR(OS.SALES_DATE, 'YYYY'), 
        TO_NUMBER(TO_CHAR(OS.SALES_DATE, 'MM')),
        GENDER
ORDER BY 1, 2, 3


