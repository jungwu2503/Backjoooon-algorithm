-- SELECT (SELECT DISTINCT CART_ID
--         FROM CART_PRODUCTS 
--         GROUP BY NAME
--         HAVING NAME = 'Milk' OR NAME = 'Yogurt') AS CART_ID
-- FROM CART_PRODUCTS 

SELECT CART_ID
FROM (SELECT DISTINCT CART_ID, NAME
    FROM CART_PRODUCTS 
    WHERE NAME IN ('Milk', 'Yogurt'))
GROUP BY CART_ID
HAVING COUNT(CART_ID) >= 2
ORDER BY CART_ID
